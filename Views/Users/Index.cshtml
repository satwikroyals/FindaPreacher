@model IEnumerable<FindaPreacher_1_.edmx.User>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<body>
    <div class="container-scroller d-flex align-items-stretch">
        <nav class="navbar default-layout col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="text-center navbar-brand-wrapper d-flex align-items-top justify-content-center">
                <a class="navbar-brand brand-logo" href="index.html">
                    <img src="~/Content/dist/images/logo_white.png" alt="FindPreacher" />
                </a>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-center">
                <div class="text-center d-lg-none d-sm-block d-md-block align-items-top justify-content-center">
                    <a class="navbar-brand brand-logo" href="index.html">
                        <img src="~/Content/dist/images/logo.png" alt="FindPreacher" width="100" />
                    </a>
                </div>
                <button type="button" id="sidebarCollapse" class="btn btn-primary d-none d-lg-block d-xl-block">
                    <i class='bx bx-menu'></i>
                    <span class="visually-hidden">Toggle Menu</span>
                </button>
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item dropdown d-xl-inline-block user-dropdown">
                        <a class="nav-link dropdown-toggle" id="UserDropdown" href="#" data-toggle="dropdown"
                           aria-expanded="false">
                            <img class="img-xs rounded-circle" src="~/Content/dist/images/face16.jpg" alt="Profile image">
                            <i class="mr-3 bx bx-menu-icon"></i>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="UserDropdown">
                            <div class="dropdown-header text-center">
                                <p class="mb-1 mt-3 font-weight-semibold">FindaPreacher</p>
                            </div>
                            <a class="dropdown-item"><i class="mr-3 bx bx-log-out"></i>Sign Out</a>
                        </div>
                    </li>
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button"
                        data-toggle="offcanvas">
                    <span class="bx bx-menu"></span>
                </button>
            </div>
        </nav>
    </div>
    <div class="container-fluid page-body-wrapper">
        <nav class="sidebar sidebar-offcanvas" id="sidebar">
            <ul class="nav">
                <li class="nav-item">
                    <a class="nav-link" href="index.html">
                        <i class="menu-icon typcn typcn-document-text"></i>
                        <span class="menu-title  nav-category">Dashboard</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-toggle="collapse" href="#ui-basic" aria-expanded="false"
                       aria-controls="ui-basic">
                        <i class="menu-icon typcn typcn-coffee"></i>
                        <span class="menu-title">Events</span>
                        <i class="bx bx-caret-down"></i>
                    </a>
                    <div class="collapse" id="ui-basic">
                        <ul class="nav flex-column sub-menu">
                            <li class="nav-item">
                                <a class="nav-link" href="/Home/Event">Upcoming Events</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/Home/Event">Favourite Events</a>
                            </li>
                        </ul>
                    </div>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="Users/Index">
                        <i class="menu-icon typcn typcn-shopping-bag"></i>
                        <span class="menu-title">Users</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="">
                        <i class="menu-icon typcn typcn-th-large-outline"></i>
                        <span class="menu-title">Preachers</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="">
                        <i class="menu-icon typcn typcn-bell"></i>
                        <span class="menu-title">About</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="">
                        <i class="menu-icon typcn typcn-user-outline"></i>
                        <span class="menu-title">Contact</span>
                    </a>
                </li>

            </ul>
        </nav>
        <div class="main-panel" id="content">
            <div class="container-xl content-wrapper">


                <div class="table-responsive shadow">
                    <div class="table-wrapper">
                        <div class="table-title">
                            <div class="row">

                                <div class="ms-auto col-9">
                                    <a href="/Users/Create" class="btn btn-secondary">
                                        <i class="bx bx-user-plus"></i>
                                        <span>Add User</span>
                                    </a>

                                </div>
                            </div>
                        </div>
                        <div class="table-filter">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="show-entries float-start">
                                        <span>Show</span>
                                        <select class="form-control custom-select shadow-sm">
                                            <option>5</option>
                                            <option>10</option>
                                            <option>15</option>
                                            <option>20</option>
                                        </select>
                                        <span>entries</span>
                                    </div>
                                    <button type="button" class="btn btn-primary">
                                        <i class="bx bx-search"></i>
                                    </button>
                                    <div class="filter-group">
                                        <label>Name</label>
                                        <input type="text" class="form-control shadow-sm">
                                    </div>
                                    <div class="filter-group">
                                        <label>Location</label>
                                        <select class="form-control custom-select shadow-sm">
                                            <option>All</option>
                                            <option>Berlin</option>
                                            <option>London</option>
                                            <option>Madrid</option>
                                            <option>New York</option>
                                            <option>Paris</option>
                                        </select>
                                    </div>
                                    <div class="filter-group">
                                        <label>Status</label>
                                        <select class="form-control custom-select shadow-sm">
                                            <option>Any</option>
                                            <option>OnGoing</option>
                                            <option>Compleated</option>

                                            <option>Cancelled</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="table-responsive">
                            <table class="table table-striped table-hover small">
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FirstName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.LastName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Email)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Mobile)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Pin)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CountryID)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Address)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Longitude)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Latitude)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.City)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.PostCode)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.ProfilePic)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.UserTypeID)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Status)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CreatedDate)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.ModifiedDate)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.DenominationTypeId)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Qualification)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Details)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.OrganisationName)
                                    </th>
                                    <th></th>
                                </tr>

                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.FirstName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.LastName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Email)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Mobile)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Pin)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CountryID)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Address)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Longitude)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Latitude)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.City)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.PostCode)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ProfilePic)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.UserTypeID)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Status)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CreatedDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ModifiedDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.DenominationTypeId)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Qualification)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Details)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.OrganisationName)
                                        </td>
                                        <td>
                                            @Html.ActionLink("Edit", "Edit", new { id = item.UserID }) |
                                            @Html.ActionLink("Details", "Details", new { id = item.UserID }) |
                                            @Html.ActionLink("Delete", "Delete", new { id = item.UserID })
                                        </td>
                                    </tr>
                                }

                            </table>

                        </div>
                    </div>

                </div>






            </div>
        </div>
    </div>
</body>
